[package]
name = "{{packageName}}"
version = "{{appVersion}}"
authors = [ "Thibault C.", "Bollard contributors" ]
description = "Models used for the Shiprs rust Docker client API"
license = "MIT"
edition = "2021"

[dependencies]
chrono = { version = "0.4", default-features = false, features = ["std", "clock", "serde"], optional = true }
serde = { version = "1.0", features = ["derive"] }
time = { version = "0.3", features = ["formatting", "parsing"], optional = true }

serde_with = {version = "3.0", default-features = false, features = ["std"]}
serde_repr = "0.1"
